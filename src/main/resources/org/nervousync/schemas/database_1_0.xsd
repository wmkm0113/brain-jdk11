    <!--    </xsd:element>-->
    <xsd:element name="brain_config">
        <xsd:annotation>
            <xsd:documentation>Data source configuration information definition</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="lazy_initialize" type="xsd:boolean" default="false">
                    <xsd:annotation>
                        <xsd:documentation>Lazy initialize data source</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="jmx_monitor" type="xsd:boolean" default="false">
                    <xsd:annotation>
                        <xsd:documentation>JMX monitor enabled status</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="ddl_type" default="NONE">
                    <xsd:annotation>
                        <xsd:documentation>Type code of entity class and database table</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="NONE"/>
                            <xsd:enumeration value="VALIDATE"/>
                            <xsd:enumeration value="CREATE"/>
                            <xsd:enumeration value="CREATE_TRUNCATE"/>
                            <xsd:enumeration value="CREATE_DROP"/>
                            <xsd:enumeration value="SYNCHRONIZE"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="schema_list">
                    <xsd:annotation>
                        <xsd:documentation>Data source configure information list</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:group ref="schema_group" maxOccurs="unbounded"/>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="storage_config" type="storage_config" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>Data import/export configure information</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>